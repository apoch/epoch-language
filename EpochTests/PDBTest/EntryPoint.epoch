test :
{
	integer count = 0
	++count
	print("test")
	display(count)
}


outer :
{
	integer whatever = 42
	display(whatever)
	test()
}

entrypoint :
{
	outer()
	display("Hello, world!")
}



display : integer stuff
{
	stuff = stuff + zero()
	print(cast(string, stuff))
}


display : string text
{
	print(text)
}


zero : -> 0

